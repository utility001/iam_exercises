{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Sid": "ListAllBucketsOnAccount",
			"Effect": "Allow",
			"Action": [
				"s3:ListAllMyBuckets"
			],
			"Resource": [
				"*"
			]
		},
		{
			"Sid": "ListAllObjectsInSpecificBucket",
			"Effect": "Allow",
			"Action": [
				"s3:ListBucket",
				"s3:DeleteObject"
			],
			"Resource": [
				"arn:aws:s3:::*spark-job*"
			]
		},
		{
			"Sid": "DenyDeleteCSVFileInDumpsFolder",
			"Effect": "Deny",
			"Action": [
				"s3:DeleteObject"
			],
			"Resource": [
				"arn:aws:s3:::spark-job-data-input/dumps/*.csv"
			]
		}
	]
}
